{
  "version": 2,
  "builds": [
    {
      "src": "blog/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "blog/wsgi.py"
    }
  ],
  "config": {
    "maxLambdaSize": "15mb",
    "runtime": "python3.9"
  }
}
